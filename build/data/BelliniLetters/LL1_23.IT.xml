						</hi>
					</s>
				</dateline>
			</div>
			<div type="letter-body" part="N" org="uniform" sample="complete">
				<salute n="ab_02">
					<s n="s_02" rend="first_line_indented" type="sentence" part="N">
						<lb facs="#LL1.23.I_line_1r_02" n="2" xml:id="LL1.23.I_lb_1r_02"/>
